{
  "name": "orgasmoproject",
  "version": "1.6.2",
  "description": "Orgasmo is a set of components and tools for developers that enhances Nextjs to help you effortless build even more outstanding web apps",
  "main": "index.js",
  "engines": {
    "node": ">=14.19.1 <=16.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "test": "./test.sh",
    "build": "npm run build --workspaces --if-present",
    "prettylint": "npm run prettylint --workspaces --if-present",
    "dev": "nodemon --exec \"npm run build && npm run dev -w\" $1",
    "preversion": "npm test",
    "postversion": "node scripts/publish.cjs"
  },
  "author": "",
  "license": "MIT",
  "workspaces": [
    "orgasmo",
    "drivers/orgasmo-filesystem",
    "drivers/orgasmo-mongo",
    "drivers/orgasmo-strapi",
    "examples/infinite",
    "create-orgasmo",
    "admin/orgasmo-strapi-admin"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "jest-environment-jsdom": "^28.1.3",
    "jest-html-reporter": "^3.7.0",
    "nodemon": "^2.0.19"
  },
  "keywords": [
    "Nextjs",
    "web",
    "webapp"
  ],
  "private": true
}
