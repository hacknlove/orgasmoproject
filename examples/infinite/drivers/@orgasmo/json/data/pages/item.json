{
  "pageId": "item",
  "patternPath": "/item/:string/:number",
  "layout": {
    "getMeta": "item.title"
  },
  "areas": {
    "header": {
      "items": [
        {
          "type": "Header",
          "props": {
            "home": true
          },
          "getProps": "item.header"
        },
        {
          "type": "Index",
          "props": {
            "links": {
              "/both": "Vertical scroll + horizontal slider.",
              "/vertical": "Vertical scroll.",
              "/horizontal": "Horizontal slider."
            }
          }
        }
      ]
    },
    "main": {
      "items": [
        {
          "type": "Item",
          "props": {
            "className": "fullWidth"
          },
          "getProps": "item.main"
        }
      ]
    },
    "footer": {
      "items": [
        {
          "type": "Index",
          "props": {
            "links": {
              "/both": "Vertical scroll + horizontal slider.",
              "/vertical": "Vertical scroll.",
              "/horizontal": "Horizontal slider."
            }
          }
        }
      ]
    }
  }
}
